<Application x:Class="Pictochat.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainView.xaml" DispatcherUnhandledException="OnDispatcherUnhandledException">
    <Application.Resources>
         <SolidColorBrush x:Key="BackgroundBrush">#fbfbfb</SolidColorBrush>
        <SolidColorBrush x:Key="TextBrush">#494949</SolidColorBrush>
        
        <FontFamily x:Key="DSFont">/Resources/#Nintendo DS BIOS</FontFamily>
    </Application.Resources>
</Application>
